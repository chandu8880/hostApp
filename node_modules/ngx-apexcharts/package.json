{
  "name": "ngx-apexcharts",
  "version": "0.8.0",
  "description": "An angular implementation of ApexCharts",
  "peerDependencies": {
    "@angular/common": ">=14.0.0",
    "@angular/core": ">=14.0.0",
    "rxjs": "^6.5.5 || ^7.4.0",
    "apexcharts": ">=3.54.0"
  },
  "keywords": [
    "angular",
    "apexcharts",
    "charts",
    "statistics",
    "ui"
  ],
  "author": {
    "name": "Morris Janatzek"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damingerdai/ngx-apexcharts/issues"
  },
  "homepage": "https://github.com/damingerdai/ngx-apexcharts",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damingerdai/ngx-apexcharts.git"
  },
  "ng-add": {
    "save": "dependencies"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "packageGroup": [
      "ngx-apexcharts"
    ],
    "migrations": "./schematics/migrations.json"
  },
  "module": "fesm2022/ngx-apexcharts.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/ngx-apexcharts.mjs"
    },
    "./signals": {
      "types": "./signals/index.d.ts",
      "default": "./fesm2022/ngx-apexcharts-signals.mjs"
    }
  },
  "sideEffects": false
}